{
  "coc.preferences.colorSupport": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "javascript",
    "json",
    "ocaml",
    "rust",
    "yaml"
  ],
  "coc.preferences.previewAutoClose": true,
  "coc.source.around.enable": false,
  "coc.source.buffer.enable": false,
  "coc.source.emoji.enable": true,
  "coc.source.file.enable": true,
  "codeLens.enable": true,
  "codeLens.separator": "â€£",
  "diagnostic.displayByAle": false,
  "diagnostic.enableMessage": "always",
  "diagnostic.enable": true,
  "diagnostic.level": "hint",
  "diagnostic.highlightOffset": 1000,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.virtualTextLines": 1,
  "diagnostic.virtualTextLineSeparator": " \\ ",
  "diagnostic.virtualTextPrefix": " ",
  "diagnostic.virtualText": true,
  "diagnostic.errorSign": "XX",
  "diagnostic.hintSign": "__",
  "diagnostic.infoSign": ">>",
  "diagnostic.warningSign": "!!",
  "eslint.autoFixOnSave": true,
  "highlight.colors.enable": true,
  "highlight.document.enable": true,
  "rust-analyzer.lens.enable": true,
  "rust-analyzer.cargo.loadOutDirsFromCheck": true,
  "rust-analyzer.procMacro.enable": true,
  "suggest.autoTrigger": "always",
  "suggest.timeout": 1000,
  "suggest.echodocSupport": true,
  "prettier.eslintIntegration": true,
  "prettier.requireConfig": true,
  "prettier.semi": false,
  "yaml.format.enable": true,
  "yaml.validate": true
}
