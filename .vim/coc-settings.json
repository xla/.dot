{
  "coc.preferences.colorSupport": true,
  "coc.preferences.formatOnSaveFiletypes": ["css", "html", "javascript", "json", "ocaml", "rust", "yaml"],
  "coc.preferences.previewAutoClose": true,
  "coc.source.around.enable": false,
  "coc.source.buffer.enable": false,
  "coc.source.emoji.enable": true,
  "coc.source.file.enable": true,
  "codeLens.enable": true,
  "codeLens.separator": "â€£",
  "diagnostic.displayByAle": false,
  "diagnostic.enableMessage": "always",
  "diagnostic.enable": true,
  "diagnostic.level": "hint",
  "diagnostic.highlightOffset": 1000,
  "diagnostic.refreshAfterSave": false,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.signOffset": 1000,
  "diagnostic.virtualTextLines": 1,
  "diagnostic.virtualTextLineSeparator": " \\ ",
  "diagnostic.virtualTextPrefix": " ",
  "diagnostic.locationlist": true,
  "diagnostic.virtualText": true,
  "diagnostic.errorSign": "XX",
  "diagnostic.hintSign": "__",
  "diagnostic.infoSign": ">>",
  "diagnostic.warningSign": "!!",
  "highlight.colors.enable": true,
  "highlight.document.enable": true,
  "languageserver": {
    "reason": {
      "command": "ocaml-language-server",
      "args": ["--stdio"],
      "filetypes": [ "ocaml", "reason" ]
    }
  },
  "suggest.autoTrigger": "always",
  "suggest.timeout": 1000,
  "suggest.echodocSupport": true,
  "eslint.enable": true,
  "prettier.eslintIntegration": true,
  "prettier.requireConfig": true,
  "prettier.semi": false,
  "yaml.format.enable": true,
  "yaml.validate": true
}
