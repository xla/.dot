{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "javascript",
    "json",
    "ocaml",
    "rust",
    "yaml"
  ],
  "coc.source.around.enable": false,
  "coc.source.buffer.enable": false,
  "coc.source.file.enable": true,
  "codeLens.enable": true,
  "codeLens.position": "eol",
  "codeLens.separator": "‣",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.displayByAle": false,
  "diagnostic.enableMessage": "always",
  "diagnostic.enable": true,
  "diagnostic.level": "hint",
  "diagnostic.messageLevel": "hint",
  "diagnostic.messageTarget": "float",
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.floatConfig": {
    "focusable": false,
    "highlight": "CocFloating"
  },
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextLevel": "hint",
  "diagnostic.virtualTextLines": 1,
  "diagnostic.virtualTextLineSeparator": " \\ ",
  "diagnostic.virtualTextPrefix": "■ ",
  "diagnostic.virtualTextWinCol": null,
  "diagnostic.errorSign": "XX",
  "diagnostic.hintSign": "__",
  "diagnostic.infoSign": ">>",
  "diagnostic.warningSign": "!!",
  "languageserver": {
		"terraform": {
			"command": "terraform-ls",
			"args": ["serve"],
			"filetypes": [
				"terraform",
				"tf"
			],
			"initializationOptions": {},
			"settings": {}
		}
	},
  "eslint.autoFixOnSave": true,
  "highlight.colors.enable": true,
  "highlight.document.enable": true,
  "prettier.requireConfig": true,
  "prettier.semi": false,
  "rust-analyzer.cargo.autoreload": true,
  "rust-analyzer.cargo.buildScripts.enable": true,
  "rust-analyzer.check.allTargets": true,
  "rust-analyzer.completion.autoimport.enable": true,
  "rust-analyzer.diagnostics.enable": true,
  "rust-analyzer.inlayHints.maxLength": 25,
  "rust-analyzer.inlayHints.renderColons": true,
  "rust-analyzer.lens.enable": true,
  "rust-analyzer.procMacro.enable": true,
  "suggest.autoTrigger": "always",
  "suggest.timeout": 1000,
  "svelte.enable-ts-plugin": true,
  "yaml.format.enable": true,
  "yaml.validate": true
}
