{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "html",
    "javascript",
    "json",
    "ocaml",
    "rust",
    "yaml"
  ],
  "coc.source.around.enable": false,
  "coc.source.buffer.enable": false,
  "coc.source.file.enable": true,
  "codeLens.enable": true,
  "codeLens.position": "eol",
  "codeLens.separator": "â€£",
  "diagnostic.displayByAle": false,
  "diagnostic.enableMessage": "always",
  "diagnostic.enable": true,
  "diagnostic.level": "hint",
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.virtualTextLines": 1,
  "diagnostic.virtualTextLineSeparator": " \\ ",
  "diagnostic.virtualTextPrefix": " ",
  "diagnostic.virtualText": true,
  "diagnostic.errorSign": "XX",
  "diagnostic.hintSign": "__",
  "diagnostic.infoSign": ">>",
  "diagnostic.warningSign": "!!",
  "languageserver": {
		"terraform": {
			"command": "terraform-ls",
			"args": ["serve"],
			"filetypes": [
				"terraform",
				"tf"
			],
			"initializationOptions": {},
			"settings": {}
		}
	},
  "rust-analyzer.cache.warmup": true,
  "rust-analyzer.callInfo.full": true,
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.cargo.runBuildScripts": true,
  "rust-analyzer.checkOnSave.allTargets": true,
  "rust-analyzer.experimental.procAttrMacros": true,
  "rust-analyzer.lens.enable": true,
  "rust-analyzer.procMacro.enable": true,
  "suggest.autoTrigger": "always",
  "suggest.timeout": 1000,
  "suggest.echodocSupport": true,
  "prettier.requireConfig": true,
  "prettier.semi": false,
  "yaml.format.enable": true,
  "yaml.validate": true
}
