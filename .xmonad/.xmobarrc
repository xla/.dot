Config
  { alignSep = "}{"
  , bgColor = "#000000"
  , border = TopB
  , borderColor = "black"
  , fgColor = "#777777"
  , font = "xft:PragmataPro:size=12,monospace:size=12"
  , position = BottomW L 100
  , sepChar = "%"
  , template = "%StdinReader%  }{ %dropbox% ┊ %multicpu% ┊ %memory% ┊ %disku% ┊ %battery% ┊ %bright% %default:Master% ┊ %wlp2s0wi%"
  , commands =
    [ Run Battery    [ "--template" , "<acstatus>"
                     , "--Low"      , "5"
                     , "--low"      , "#b03060"
                     , "--" -- battery specific options
                           -- discharging status
                           , "-o"	, "<leftvbar>"
                           -- AC "on" status
                           , "-O"	, "<fc=#ffe4b5><leftvbar></fc>"
                           -- charged status
                           , "-i"	, "<fc=#66cdaa><leftvbar></fc>"
                     ] 100
    , Run Brightness [ "-t", "<vbar>", "--", "-D", "intel_backlight" ] 10
    , Run Com        "xmobar-dropbox-status" [] "dropbox" 100
    , Run MultiCpu   [ "-t"
                     , "<vbar0> <vbar1> <vbar1> <vbar3> <vbar4> <vbar5> <vbar6> <vbar7>"
                     ]
                     10
    , Run DiskU      [ ( "nvme0n1p2", "<freevbar>" )
                     , ( "nvme0n1p3", "<freevbar>" )
                     , ( "nvme0n1p4", "<freevbar>" )
                     ]
                     [ "-L", "20", "-H", "50", "-m", "1", "-p", "3" ]
                     100
    , Run Memory     [ "-t", "<availablevbar>" ] 50
    , Run StdinReader
    , Run Volume     "default" "Master" [ "-t", "<volumevbar>" ] 10
    , Run Wireless   "wlp2s0" [ "-t", "<qualityvbar> <essid>"] 10
    ]
  }
